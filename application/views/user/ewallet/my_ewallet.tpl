{include file="user/layout/header.tpl"  name=""}<div id="span_js_messages" style="display:none;">    <span id="error_msg">{$tran_you_must_enter_user_name}</span>            <span id="row_msg">{$tran_rows}</span>    <span id="show_msg">{$tran_shows}</span>    <input type="hidden" id="path_temp" name="path_temp" value="{$PUBLIC_URL}">    <input type="hidden" id="path_root" name="path_root" value="{$PATH_TO_ROOT_DOMAIN}"></div><div class="row">    <div class="col-sm-12">        <div class="panel panel-default">            <div class="panel-heading">                <i class="fa fa-external-link-square"></i>{$tran_my_ewallet_details}                <div class="panel-tools">                    <a class="btn btn-xs btn-link panel-collapse collapses" href="#">                    </a>                    <a class="btn btn-xs btn-link panel-refresh" href="#">                        <i class="fa fa-refresh"></i>                    </a>                    <a class="btn btn-xs btn-link panel-expand" href="#">                        <i class="fa fa-resize-full"></i>                    </a>                    <a class="btn btn-xs btn-link panel-close" href="#">                        <i class="fa fa-times"></i>                    </a>                </div>            </div>            <div class="panel-body">                {assign var=count value=""}                {assign var=i value="0"}                {assign var=amount value=""}                {assign var=date value=""}                {assign var=amount_type value=""}                {assign var=class value=""}                {assign var=type value=""}                {assign var=balance value=""}                {assign var=total_debit value=""}                {assign var=total_credit value=""}                {if $details_count>0}                    {$total_debit = 0}                    {$total_credit = 0}	                    <center><h3>{$tran_ewallet_details} : {$user_name} </h3></center>                    <table class="table table-striped table-bordered table-hover table-full-width" id="grid">                        <thead>                            <tr class="th" align="center">                                <th>{$tran_slno}</th>                                <th>{$tran_date}</th>                                <th>{$tran_description}</th>                                <th>{$tran_amount}</th>                                <th>{$tran_balance}</th>                            </tr>                        </thead>                        <tbody>                            {$balance = 0}                            {foreach from=$details item=v}                                {$amount = $v.total_amount}                                {$date = $v.date}                                {$amount_type = $v.amount_type}                                {if $amount_type == "user_credit"}                                    {assign var=from_usr_name value= $v.from_user_name}                                    {$type ="Transfer From  $from_usr_name"}                                    {$balance = $balance + $amount}                                {else if $amount_type == "user_debit"}                                    {assign var=from_usr_name value= $v.from_user_name}                                        {$type="Transfer To  $from_usr_name"}                                    {$balance = round($balance - $amount,2)}                                    {$amount = "-"+$amount}                                {else if $amount_type == "admin_debit"}                                    {assign var=from_usr_name value= $v.from_user_name}                                        {$type="Deducted By  $from_usr_name"}                                    {$balance = round($balance - $amount,2)}                                    {$amount = "-"+$amount}                                {else if $amount_type == "admin_credit"}                                    {assign var=from_usr_name value= $v.from_user_name}                                        {$type="Credited By  $from_usr_name"}                                    {$balance = $balance + $amount}                                {else if $amount_type == "referal"}                                    {$type = "Referral commission"}                                    {$balance = $balance + $amount}                                    {$amount = "+"+$amount}                                {else if $amount_type == "payout_released"}                                    {$type = "Payout Released"}                                    {$balance = round($balance - $amount,2)}                                    {$amount = "-"+$amount}                                {else if $amount_type == "pin_purchased"}                                    {$type = "Pin Purchased"}                                    {$balance = round($balance - $amount,2)}                                    {$amount = "-"+$amount}                                {else if $amount_type == "leg"}                                    {$type = "Binary Commission"}                                    {$balance = $balance + $amount}                                {else if $amount_type == "auto_board_1"}                                    {$type = "Board Commission"}                                    {$balance = $balance + $amount}                                {else if $amount_type == "fsb"}                                    {$type = "Fast Start Bonus"}                                    {$balance = $balance + $amount}                                {else if $amount_type == "annual_fee"}                                    {$type = "Annual Fee"}                                    {$balance = $balance - $amount}                                {else if $amount_type == "registration"}                                    {$type = "Ewallet Registration"}                                    {$balance = $balance - $amount}                                {else}                                     {$type = $amount_type}                                    {$balance = $balance + $amount}                                {/if}                                {if $i%2 == 0}                                    {$class="tr2"}                                {else}                                    {$class="tr1"}                                {/if}		                                {$i = $i+1}                                <tr class="{$class}" align="center" >                                    <td>{counter}</td>                                    <td>{$date}</td>                                    <td>{$type}</td>                                    <td>{number_format($amount,2)}</td>                                    <td>{number_format($balance,2)}</td>                                </tr>                            {/foreach}                               <tr class="th">                                <td colspan="4" align="right"><b>{$tran_available_amount} </b></td>                                <td align="center"><b>{number_format($balance,2)}</b></td>                            </tr>							                        </tbody>                    </table>                    <table>                    </table>                {else}                    <h3> {$tran_no_transfer_details}</h3>                {/if}            </div>        </div>    </div></div>          {include file="user/layout/footer.tpl" title="Example Smarty Page" name=""}{if $details_count>0}    <script>        jQuery(document).ready(function() {            Main.init();            TableData.init();            ValidateUser.init();        });    </script>{else}    <script>        jQuery(document).ready(function() {            Main.init();            ValidateUser.init();        });    </script>{/if}{include file="user/layout/page_footer.tpl" title="Example Smarty Page" name=""}